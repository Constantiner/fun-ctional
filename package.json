{
	"name": "@constantiner/fun-ctional",
	"version": "0.6.2",
	"description": "The library brings most of the familiar functional techniques (like functional composition) to asynchronous world with shining Promises",
	"main": "fun-ctional",
	"scripts": {
		"test": "node -r esm scripts/unitTest.js",
		"lint": "npx eslint '**/*.js'",
		"build": "npx gulp clean && npx gulp es6modules && npx gulp es5modules && npx gulp es5modulesMin",
		"format": "npx prettier 'src/*.js' --write",
		"prepublishOnly": "npm run clean && npm run lint && npm run test && npm run build",
		"clean": "npx gulp clean",
		"clear_jest": "npx jest --clearCache",
		"lint-staged": "lint-staged",
		"validate:sourcemaps": "node -r esm scripts/sourcemapsValidator.js",
		"test:coverage": "npm test -- --coverage",
		"test:watch": "node -r esm scripts/unitTestWatch.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Constantiner/fun-ctional.git"
	},
	"keywords": [
		"JS",
		"JavaScript",
		"Functional",
		"FP",
		"Asynchronous",
		"Utility",
		"Utilities",
		"ES6",
		"Promise"
	],
	"author": {
		"name": "Konstantin Kovalev",
		"email": "constantiner@gmail.com"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Constantiner/fun-ctional/issues"
	},
	"homepage": "https://github.com/Constantiner/fun-ctional#readme",
	"engines": {
		"node": ">=7.6",
		"npm": ">=5.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.4.4",
		"@babel/preset-env": "^7.4.4",
		"@constantiner/resolve-node-configs-hierarchy": "^0.4.0",
		"@types/gulp": "^4.0.6",
		"@types/jest": "^24.0.13",
		"babel-jest": "^24.8.0",
		"date-fns": "^1.30.1",
		"del": "^4.1.1",
		"eslint": "^5.16.0",
		"eslint-config-prettier": "^4.2.0",
		"eslint-plugin-jest": "^22.5.1",
		"eslint-plugin-prettier": "^3.1.0",
		"eslint-plugin-unicorn": "^8.0.2",
		"esm": "^3.2.22",
		"globby": "^9.2.0",
		"gulp": "^4.0.2",
		"gulp-better-rollup": "^4.0.1",
		"gulp-rename": "^1.4.0",
		"gulp-sourcemaps": "^2.6.5",
		"gulp-uglify": "^3.0.2",
		"husky": "^2.3.0",
		"jest": "^24.8.0",
		"lint-staged": "^8.1.6",
		"prettier": "^1.17.1",
		"rollup": "^1.11.3",
		"rollup-plugin-babel": "^4.3.2",
		"sourcemap-validator": "^1.1.0"
	},
	"dependencies": {}
}
