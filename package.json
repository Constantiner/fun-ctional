{
	"name": "@constantiner/fun-ctional",
	"version": "0.6.2",
	"description": "The library brings most of the familiar functional techniques (like functional composition) to asynchronous world with shining Promises",
	"main": "fun-ctional",
	"scripts": {
		"test": "npx jest --config jest.config.json",
		"lint": "npx eslint '**/*.js'",
		"build": "npx gulp clean && npx gulp es6modules && npx gulp es5modules && npx gulp es5modulesMin",
		"format": "npx prettier 'src/*.js' --write",
		"prepublishOnly": "npm run clean && npm run lint && npm run test && npm run build",
		"clean": "npx gulp clean",
		"clear_jest": "npx jest --clearCache"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Constantiner/fun-ctional.git"
	},
	"keywords": [
		"JS",
		"JavaScript",
		"Functional",
		"FP",
		"Asynchronous",
		"Utility",
		"Utilities",
		"ES6",
		"Promise"
	],
	"author": "Konstantin Kovalev <constantiner@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Constantiner/fun-ctional/issues"
	},
	"homepage": "https://github.com/Constantiner/fun-ctional#readme",
	"prettier": {
		"printWidth": 120,
		"useTabs": true,
		"tabWidth": 4
	},
	"engines": {
		"node": ">=7.6",
		"npm": ">=5.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.1.2",
		"@babel/preset-env": "^7.0.0",
		"@types/gulp": "^4.0.5",
		"@types/jest": "^23.3.5",
		"babel-core": "^7.0.0-bridge.0",
		"babel-jest": "^23.6.0",
		"del": "^3.0.0",
		"eslint": "^5.7.0",
		"eslint-config-prettier": "^3.1.0",
		"gulp": "^4.0.0",
		"gulp-better-rollup": "^3.3.0",
		"gulp-rename": "^1.4.0",
		"gulp-sourcemaps": "^2.6.4",
		"gulp-uglify": "^3.0.1",
		"jest": "^23.6.0",
		"jest-pnp-resolver": "^1.0.1",
		"prettier": "^1.14.3",
		"regenerator-runtime": "^0.12.1",
		"rollup-plugin-babel": "^4.0.1"
	},
	"dependencies": {}
}
